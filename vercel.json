{
    "version": 2,
    "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
      {
        "src": "tailwind.config.js",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.html"
      }
    ],
    "buildCommand": "npx tailwindcss -i ./src/input.css -o ./public/stylesheets/output.css"
  }
  